    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001e9d40068e0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 83 "$display", "Time=%0t: S4 (Failsafe Power Fail)", $time {0 0 0};
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001e9d40068e0_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 89 "$display", "Time=%0t: S0 (Standby) - PLN Pulih", $time {0 0 0};
    %delay 50000, 0;
    %vpi_call 2 93 "$display", "Simulasi selesai pada waktu %0t ns", $time {0 0 0};
    %vpi_call 2 94 "$finish" {0 0 0};
    %end;
    .thread T_6;
